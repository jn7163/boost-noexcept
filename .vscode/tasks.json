{
    "displayName": "All unit tests",
    "description": "Unit test",
    "isBuildCommand": "true",
    "command": "cmd",
    "args": [
        "/C",
        "cd ${workspaceRoot}/bld/debug && mesontest.py"
    ],
    "version": "0.1.0",
    "problemMatcher": {
        "fileLocation": [
            "relative",
            "${workspaceRoot}/bld/debug"
        ],
        "owner": "cpp",
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+.*(warning|error|note):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    },
    "chooserIndex": 0
}